{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDataset_rwb",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {#} as string,",
				"          Occupation as string,",
				"          {Median wage (US)} as string,",
				"          {Projected growth (by 2031)} as string,",
				"          {Risk level} as string,",
				"          {Risk level (voted)} as string,",
				"          {Job score} as string,",
				"          {Popularity(100 is most viewed)} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1"
			]
		}
	}
}